<div class="row justify-content-end">
    <a href="/images/json/allcovers/allcategory/true" title="Code images cover '{{this.modelName}}'" class="sync"
        target="_blank"><i class="fas fa-code" aria-hidden="true"></i></a>
    <a href="/images/json/allcovers/allcategory/false" title="Sent images cover '{{this.modelName}}' to MySales"
        class="sync"><i class="fas fa-paper-plane" aria-hidden="true"></i></a>
</div>

        {{#each yearCatalog}}
            <h1 class="txtFeature">{{this.codeYear}}</h1>
            <br/>
            <div class="col-md-12">
                <div class="row">
                    {{#each ../images}}
                        {{#if_equal this.year ../this.codeYear}}
                            {{#each ../../vehicleCatalog}}
                                {{#if_equal this.year ../this.year}}
                                    {{#if_equal this.codeHtml ../this.model}}
                                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3">
                                            <div class="card" style="width: 24rem;">
                                                <a href="/images/{{this.codeHtml}}">
                                                    <img class="card-img-top" src="{{../this.url}}" alt="Card image cap">
                                                </a>
                                                <div class="card-body footer-cover">
                                                     <p class="card-title">{{this.name}}</p>
                                                    <h5 class="card-text">{{../this.description}}</h5>
                                                    <a href="/images/{{../this.model}}" class="btn btn-primary">Go</a>
                                                </div>
                                            </div>
                                        </div>
                                    {{/if_equal}}
                                {{/if_equal}}          
                            {{/each}}  
                        {{/if_equal}}                    
                    {{/each}}
                </div>
            </div>
            <br/>
            <br/>
            <br/>
            <br/>
        {{/each}} 

